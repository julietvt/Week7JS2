задание 9
у вас программа находит все простые числа внутри введенного
это почти да.но нужен флаг например , который при его наличии (флага)сразу позвонит сказать чтооно простое
let n=+prompt();
let i, flag=false;
if(n==1) flag=true;
for (i=2; i<n; i++){
   if(n%i == 0) {
     flag=false;
     break;
   }
   else{
     flag=true;
     break;
   }
}
if (flag==true) alert('prostoe');
else alert('neprostoe');

